sig_stars = function(p)
{
stars = symnum(p, na = F, cutpoints = c(0, .001, .01, .05, .1, 1), symbols=c("***","**", "*", ".", " "))
return( paste(round(p, 3), stars) )
}
t_table = apply(tt$p.value, c(1,2), sig_stars)
t_table
